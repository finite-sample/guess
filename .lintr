cache_directory: ".lintr_cache"
linters: linters_with_defaults(
    line_length_linter = line_length_linter(100),
    object_length_linter = object_length_linter(40),
    commented_code_linter = NULL,
    trailing_whitespace_linter = NULL,
    indentation_linter = indentation_linter(2),
    assignment_linter = NULL,
    object_name_linter = object_name_linter(styles = c("snake_case", "dotted.case"))
 )
exclusions: list(
    "data-raw" = list(
        trailing_whitespace_linter = Inf,
        line_length_linter = Inf
    ),
    "vignettes" = list(
        line_length_linter = Inf
    ),
    "tests/testthat.R" = Inf,
    "tests/_covrpage.Rmd" = Inf,
    "R/RcppExports.R" = Inf
 )
